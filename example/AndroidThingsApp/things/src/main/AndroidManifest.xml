<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.estimote.estimotethings">

    <application>
        <uses-library android:name="com.google.android.things"/>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.IOT_LAUNCHER"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name=".LightLevelDemoActivity">
        </activity>
        <activity android:name=".TemperatureDemoActivity">
        </activity>
        <activity android:name=".MotionDemoActivity">
        </activity>
        <activity android:name=".ProximityDemoActivity">
        </activity>
    </application>

</manifest>